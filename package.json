{
  "name": "wilodevapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "app:clean": "sudo rm -rf node_modules bun.lockb && bun install",
    "setDevelopment": "ENVFILE=.env.development",
    "setStaging": "ENVFILE=.env.staging",
    "setProduction": "ENVFILE=.env.production",
    "ios:Pod:Reset": "cd ios && pod deintegrate && pod setup && pod install",
    "ios:Pod:install": "cd ios && pod install && cd ..",
    "ios:bundle:assets": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:clean": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && sudo rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd .. && bun ios:Pod:Reset",
    "ios:dev:clean": "bun ios:clean && bun setDevelopment  react-native run-ios --mode=Debug --scheme \"development\"",
    "ios:dev": "bun setDevelopment  react-native run-ios --mode=Debug --scheme \"development\"",
    "ios:staging": "bun ios:clean && bun setStaging  react-native run-ios --mode=Debug --scheme \"staging\"",
    "ios:prod": "bun ios:clean &&  bun setProduction  react-native run-ios --mode=Debug --scheme \"production\"",
    "aos:clean": "bun node:clean && cd android && ./gradlew clean",
    "aos:bundle:assets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "aos:remove:assets": "rm -rf android/app/src/main/res/drawable-hdpi &&  rm -rf android/app/src/main/res/drawable-mdpi &&  rm -rf android/app/src/main/res/drawable-xhdpi && rm -rf android/app/src/main/res/drawable-xxhdpi && rm -rf android/app/src/main/res/drawable-xxxhdpi && rm -rf android/app/src/main/res/raw",
    "aos:dev": "bun setDevelopment  react-native run-android --deviceId= \"aPhone 33\" --mode=developmentDebug --appIdSuffix=development",
    "aos:dev-release": "bun setDevelopment  react-native run-android --deviceId= \"aPhone 33\" --mode=developmentRelease--appIdSuffix=development",
    "aos:staging": "bun setStaging  react-native run-android --deviceId= \"aPhone 33\" --mode=stagingDebug --appIdSuffix=staging",
    "aos:staging-release": "bun setStaging  react-native run-android --deviceId= \"aPhone 33\" --mode=stagingRelease --appIdSuffix=staging",
    "aos:prod": "bun setProduction  react-native run-android --deviceId= \"aPhone 33\" --mode=productionDebug --appIdSuffix=production",
    "aos:prod-release": "bun setProduction  react-native run-android --deviceId= \"aPhone 33\" --mode=productionRelease --appIdSuffix=production",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prettier": "prettier --check '**/*.{ts,tsx,json}'",
    "prettier:fix": "prettier --write '**/*.{ts,tsx,json}'",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest .",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage --coverageDirectory=\"./coverage\"",
    "postinstall": "bunx patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.52",
    "@gluestack-ui/config": "latest",
    "@gluestack-ui/themed": "^1.1.17",
    "@hookform/resolvers": "^3.3.4",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/material-top-tabs": "6.6.13",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "@reduxjs/toolkit": "^2.2.2",
    "axios": "^1.6.8",
    "i18next": "^23.10.1",
    "react": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.6",
    "react-native-config": "^1.5.1",
    "react-native-pager-view": "^6.2.3",
    "react-native-reanimated": "^3.8.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^15.1.0",
    "react-redux": "^9.1.0",
    "yup": "^1.4.0",
    "yup-password": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-private-methods": "^7.24.1",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime": "^7.24.1",
    "@react-native/babel-preset": "0.75.0-main",
    "@react-native/eslint-config": "0.75.0-main",
    "@react-native/metro-config": "0.75.0-main",
    "@react-native/typescript-config": "0.75.0-main",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.2.2",
    "@testing-library/react-native": "^12.4.4",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "react-native-svg-asset-plugin": "^0.5.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=20"
  }
}
